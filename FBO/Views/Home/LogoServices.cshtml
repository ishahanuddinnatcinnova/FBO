<style>


    .inputCheckbox input {
        -webkit-appearance: none;
        position: relative;
        width: 100%;
        cursor: pointer;
        border: none
    }

        .inputCheckbox input:after {
            position: absolute;
            font-family: "Font Awesome 5 Free";
            font-weight: 400;
            content: "\f111";
            font-size: 18px;
            color: #478bfb;
            right: 10px;
            top: -5px;
        }

        .inputCheckbox input:checked:after {
            font-weight: 900;
            content: "\f058";
            color: #478bfb;
        }

    .container2 img {
        width: 80%;
        position: absolute;
        margin: auto;
        left: 0;
        right: 0;
        top: 0;
        bottom: 0;
        cursor: pointer;
    }

    @@media screen and (min-width: 950px) {
        .wrapper2 {
            grid-template-columns: repeat(4, 1fr);
            gap: 10px;
        }

        .container2 {
            height: 160px;
            width: 90%;
        }

        input[type="checkbox"]:after {
            font-size: 22px;
        }
    }

    .section-title {
        position: relative;
        text-transform: uppercase;
        margin-bottom: 1.875rem;
    }

        .section-title::before {
            position: absolute;
            left: 0;
            bottom: -8px;
            content: "";
            height: 2px;
            width: 5rem;
            background-color: #97cb00;
        }

    .container2:hover {
        transform: scale(1.05);
        box-shadow: 10px 10px 10px #888888;
    }

</style>
