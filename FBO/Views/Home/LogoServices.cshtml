@section Scripts{
    <script>
        if ($('#s').show())
            setTimeout(function () {
                $('#s').hide();
            }, 4000);


        else if ($('#f').show()) {


            setTimeout(function () {
                $('#f').hide();
            }, 4000);
        }
        var max_limit = 10; // Max Limit
        $(document).ready(function () {
            var limit = 0;
        @if (Model.FBO.FBO.FBOLevel == "Platinum")
        {
            <text>
                    limit=10;
            </text>
        }
        else if (Model.FBO.FBO.FBOLevel == "Gold")
        {
            <text>
                    limit=6;
            </text>
        }
        else if (Model.FBO.FBO.FBOLevel == "Silver")
        {
            <text>
                    limit=1;
            </text>
        }
        else if (Model.FBO.FBO.FBOLevel == "Basic")
        {
            <text>
                    limit=0;
            </text>
        }
                $("#logoser").submit(function (e) {
                    if ($(":checkbox:checked").length > limit) {
                        e.preventDefault();
                        $('.toast').toast('show');
                        setTimeout(function () {
                            $('.toast').toast('hide');
                        }, 4000);
                    }
                });
            $('.limit').text(limit);
            $("#logoser").submit(function (e) {
                if ($(":checkbox:checked").length > limit) {
                    e.preventDefault();

                    $('#selectCount').text($(":checkbox:checked").length);

                    $('.toast').toast('show');
                    setTimeout(function () {
                        //$('.toast').toast('hide');
                    }, 4000);
                }
            });
        });


    </script>
}






