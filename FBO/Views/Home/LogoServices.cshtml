            color: #97cb00;
            border-bottom: #97cb00 2px solid;
        }

    label {
        font-weight: 600;
    }

    .wrapper2 {
        width: 100%;
        display: grid;
        place-items: center;
        gap: 15px;
    }

    .container2 {
        height: 150px;
        width: 30%;
        background-color: #ffffff;
        box-shadow: 0 0 25px rgba(17, 1, 68, 0.08);
        border-radius: 8px;
        position: relative;
        cursor: pointer;
    }

    .inputCheckbox input {
        -webkit-appearance: none;
        position: relative;
        width: 100%;
        cursor: pointer;
        border: none
    }

        .inputCheckbox input:after {
            position: absolute;
            font-family: "Font Awesome 5 Free";
            font-weight: 400;
            content: "\f111";
            font-size: 18px;
            color: #478bfb;
            right: 10px;
            top: -5px;
        }

        .inputCheckbox input:checked:after {
            font-weight: 900;
            content: "\f058";
            color: #478bfb;
        }

    .container2 img {
        width: 80%;
        position: absolute;
        margin: auto;
        left: 0;
        right: 0;
        top: 0;
        bottom: 0;
        cursor: pointer;
    }

    @@media screen and (min-width: 950px) {
        .wrapper2 {
            grid-template-columns: repeat(4, 1fr);
            gap: 10px;
        }

        .container2 {
            height: 160px;
            width: 90%;
        }

        input[type="checkbox"]:after {
            font-size: 22px;
        }
    }

    .section-title {
        position: relative;
        text-transform: uppercase;
        margin-bottom: 1.875rem;
    }

        .section-title::before {
            position: absolute;
            left: 0;
            bottom: -8px;
            content: "";
            height: 2px;
            width: 5rem;
            background-color: #97cb00;
        }

    .container2:hover {
        transform: scale(1.05);
        box-shadow: 10px 10px 10px #888888;
    }

    .toast {
        width: 450px !important;
        margin: auto;
        margin-top: 2rem;
        position: fixed;
        top: 20px;
        z-index: 1;
        left: 34%;
    }



    .toast-content {
        padding: 15px;
    }



    .icon i {
        font-size: 35px;
    }




    .toast.success {
        border: 2px solid #14A44D !important;
        color: #14A44D;
    }



    .toast.warning {
        border: 2px solid #F9A02C !important;
        color: #F9A02C;
    }



    .toast.erro {
        border: 2px solid #D90429 !important;
        color: #D90429;
    }
